<?xml version="1.0"?>
<config>
    <listaDrones>
        <dron> A01 </dron>
        <dron> A02 </dron>
        <dron> A03 </dron>
        <dron> A04 </dron>
    </listaDrones>
    <listaSistemasDrones>
        <sistemaDrones nombre="NombreSistemaDrones">
            <alturaMaxima>7</alturaMaxima>
            <cantidadDrones>4</cantidadDrones>
            <contenido>
                <dron> A01 </dron>
                <alturas>
                    <altura valor="valorAltura"> 2 </altura>
                </alturas>
            </contenido>
            <contenido>
                <dron> A02 </dron>
                <alturas>
                    <altura valor="valorAltura"> 4 </altura>
                </alturas>
            </contenido>
            <contenido>
                <dron> A03 </dron>
                <alturas>
                    <altura valor="valorAltura"> 5 </altura>
                </alturas>
            </contenido>
            <contenido>
                <dron> A04 </dron>
                <alturas>
                    <altura valor="valorAltura"> 7 </altura>
                </alturas>
            </contenido>
        </sistemaDrones>
    </listaSistemasDrones>
    <listaMensajes>
        <Mensaje nombre="NombreMensaje">
            <sistemaDrones>[valorAlfanumerico]</sistemaDrones>
            <instrucciones>
                <instruccion dron="NombreDron">[valorNumericoAltura]</instruccion>
            </instrucciones>
        </Mensaje>
    </listaMensajes>
</config>